stable.melpa.org {
  gzip {
    ext *
    level 3
  }
  root /home/melpa/melpa/html
  tls d@milkbox.net
  log /home/melpa/log/melpa.access.log {
    rotate {
      size 10
    }
  }
  errors /home/melpa/log/melpa.error.log
  header / Strict-Transport-Security "max-age=31536000;"
}

stable.melpa.org:80 {
  gzip {
    ext *
    level 3
  }
  root /home/melpa/melpa/html
  log /home/melpa/log/melpa.access.log
  errors /home/melpa/log/melpa.error.log
}
